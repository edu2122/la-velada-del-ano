<html lang="es">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<title>La Velada del ano, proyecto de Ibai</title>
	</head>
	<body>
		<header></header>
		<main>
			<section
				id="boxers"
				class={`flex justify-between relative
						[&>div>img]:absolute [&>div>img]:bottom-0
						[&>div>img]:w-auto
						h-[40vh] md:h-[65vh] lg:h-screen pt-4 lg:pt-24 border-b border-white/10 overflow-hidden
				`}
			>
				<div class="relative w-full">
					<img 
						class="left-0 h-full z-50" 
						src="/boxers/coscu.webp" 
						alt="coscu"
					/>
					<img 
						class="left-[5%] h-[80%] z-40" 
						src="/boxers/mayichi.webp" 
						alt="mayichi"
					/>
					<img 
						class="left-[15%] h-[75%] z-30" 
						src="/boxers/viruzz.webp" 
						alt="viruzz"
					/>
					<img 
						class="left-[27%] h-[70%] z-20" 
						src="/boxers/fernanfloo.webp" 
						alt="fernanfloo"
					/>
					<img 
						class="left-[35%] h-[68%] z-10" 
						src="/boxers/la-rivers.webp" 
						alt="la-rivers"
					/>
					<img 
						class="left-[40%] h-[61%] z-0" 
						src="/boxers/papigavi.webp" 
						alt="papigavi"
					/>
				</div>
				<div class="relative w-full">
					<img 
						class="right-0 h-full z-50" 
						src="/boxers/german.webp" 
						alt="German"
					/>
					<img 
						class="right-[5%] h-[80%] z-40" 
						src="/boxers/amouranth.webp"
						alt="Amourtanth"
					/>
					<img 
						class="right-[15%] h-[75%] z-30" 
						src="/boxers/shelao.webp" 
						alt="Shelao"
					/>
					<img 
						class="right-[28%] h-[70%] z-20" 
						src="/boxers/luzu.webp" 
						alt="Luzu"
					/>
					<img 
						class="right-[35%] h-[68%] z-10" 
						src="/boxers/rivers.webp" 
						alt="Rivers"
					/>
					<img 
						class="right-[40%] h-[63%] z-0" 
						src="/boxers/ampeter.webp" 
						alt="Ampeter"
					/>
				</div>
			</section>
		</main>
	</body>
</html>
